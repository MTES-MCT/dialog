<?xml version="1.0" encoding="UTF-8"?>
<constraint-mapping xmlns="http://symfony.com/schema/dic/constraint-mapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://symfony.com/schema/dic/constraint-mapping https://symfony.com/schema/dic/constraint-mapping/constraint-mapping-1.0.xsd">
    <class name="App\Application\RegulationOrder\Command\CreateRegulationOrderCommand">
        <property name="description">
            <constraint name="NotBlank" />
        </property>
        <property name="issuingAuthority">
            <constraint name="NotBlank" />
        </property>
        <property name="startPeriod">
            <constraint name="NotBlank" />
            <constraint name="Type">
                <option name="type">\DateTimeInterface</option>
            </constraint>
        </property>
        <property name="endPeriod">
            <constraint name="Type">
                <option name="type">\DateTimeInterface</option>
            </constraint>
            <constraint name="GreaterThan">
                <option name="propertyPath">startPeriod</option>
            </constraint>
        </property>
        <property name="maxWeight">
            <constraint name="Positive" />
        </property>
        <property name="maxHeight">
            <constraint name="Positive" />
        </property>
        <property name="maxWidth">
            <constraint name="Positive" />
        </property>
        <property name="maxLength">
            <constraint name="Positive" />
        </property>
    </class>
</constraint-mapping>
