            <trans-unit id="common.error.not_blank">
                <source>common.error.not_blank</source>
                <target>Cette valeur ne doit pas être vide.</target>
            </trans-unit>
            <trans-unit id="regulation.location.error.geocoding_failed">
                <source>regulation.location.error.geocoding_failed</source>
                <target>Cette adresse n’est pas reconnue. Vérifier le nom de la voie, et les numéros de début et fin.</target>
            </trans-unit>
            <trans-unit id="regulation.location.error.departmental_road_geocoding_failed">
                <source>regulation.location.error.departmental_road_geocoding_failed</source>
                <target>La géolocalisation de la départementale entre ces points de repère a échouée. Veuillez vérifier que ces PR appartiennent bien à une même portion de voie.</target>
            </trans-unit>
            <trans-unit id="invalid.page_or_page_size">
                <source>invalid.page_or_page_size</source>
                <target>Le nombre d'éléments par page et/ou le numéro de la page doivent être supérieur à 0.</target>
            </trans-unit>
            <trans-unit id="regulation.detail.regulation_cant_be_published">
                <source>regulation.detail.regulation_cant_be_published</source>
                <target><![CDATA[<h3 class="fr-alert__title">]]>La réglementation ne peut pas être publiée. Vérifiez qu'elle a correctement été créée.<![CDATA[</h3>]]></target>
            </trans-unit>
            <trans-unit id="regulation.error.end_date_before_start_date">
                <source>regulation.error.end_date_before_start_date</source>
                <target>La date de fin doit être après le {{ compared_value }}.</target>
            </trans-unit>
            <trans-unit id="regulation.step1.error.identifier">
                <source>regulation.step1.error.identifier</source>
                <target>L'identifiant est déjà utilisé sur un autre arrêté.</target>
            </trans-unit>
            <trans-unit id="regulation.general_info.error.identifier">
                <source>regulation.general_info.error.identifier</source>
                <target>Un arrêté avec cet identifiant existe déjà. Veuillez saisir un autre identifiant.</target>
            </trans-unit>
            <trans-unit id="measure.locations.error.not_blank">
                <source>measure.locations.error.not_blank</source>
                <target>Veuillez définir une ou plusieurs localisations.</target>
            </trans-unit>
            <trans-unit id="location.isEntireStreet.error.need_house_numbers">
                <source>location.isEntireStreet.error.need_house_numbers</source>
                <target>Veuillez définir le numéro de début et/ou le numéro de fin.</target>
            </trans-unit>
            <trans-unit id="regulation.vehicle_set.restricted_types.error.not_blank">
                <source>regulation.vehicle_set.restricted_types.error.not_blank</source>
                <target>Veuillez spécifier un ou plusieurs véhicules concernés.</target>
            </trans-unit>
            <trans-unit id="regulation.vehicle_set.dimensions.error.not_blank">
                <source>regulation.vehicle_set.dimensions.error.not_blank</source>
                <target>Veuillez spécifier le gabarit des véhicules concernés.</target>
            </trans-unit>
            <trans-unit id="regulation.period.error.end_date">
                <source>regulation.period.error.end_date</source>
                <target>La date de fin doit être supérieure à la date de début.</target>
            </trans-unit>
