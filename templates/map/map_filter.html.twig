<dialog-auto-form>
    {{ form_start(form, {attr: {'data-turbo-action' : 'advance'}}) }}
	{{ form_row(form.category, {group_class: 'fr-radio-group'}) }}
	{{ form_row(form.displayFutureRegulations, {group_class: 'fr-checkbox-group fr-pl-1w fr-pb-1w'}) }}
	{{ form_row(form.displayPastRegulations, {group_class: 'fr-checkbox-group fr-pl-1w'}) }}
	<noscript>
	    {{ form_row(form.save) }}
	</noscript>
    {{ form_end(form) }}
</dialog-auto-form>
