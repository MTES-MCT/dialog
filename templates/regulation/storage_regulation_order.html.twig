<section class="fr-container fr-py-5w">
    <h1 class="fr-h3">{{ 'regulation.storage.title' |trans }}</h1>
    {{ form_start(form, { attr: { 'data-controller': 'form-submit' }}) }}
        <fieldset
            class="fr-fieldset"
            role="radiogroup"
            aria-labelledby=""
            data-controller="form-reveal"
        >
            {% for option in form.allRegulationOrderStorage %}
                <div class="fr-fieldset__element">
                    <div class="fr-radio-group fr-radio-rich">
                        {{ form_widget(option, {
                            attr: {
                                'data-controller': 'condition',
                                'data-condition-equals-value': '' ~ option.vars.value ~ '',
                                'data-action': 'change->condition#dispatchFromInputChange condition:yes->form-reveal#open condition:no->form-reveal#close',
                            },
                        }) }}
                        {{ form_label(option, null, { help: (option.vars.label ~ '.help')|trans }) }}
                        <div class="fr-radio-rich__pictogram">
                            <svg class="fr-artwork" aria-hidden="true" viewBox="0 0 26 26" width="26px" height="26px">
                                <use class="fr-artwork-major" xlink:href="{{ asset('images/regulation_order.' ~ option.vars.value ~ '.svg') }}#artwork-major"></use>
                            </svg>
                        </div>
                    </div>
                </div>
            {% endfor %}

            <div class="fr-fieldset__element" data-form-reveal-target="section" {% if form.allRegulationOrderStorage.vars.value != 'file' %}hidden{% endif %}>
                <div class="app-card app-card--content-only app-someVehicles__form">
                    <div class="app-card__content">
                        <fieldset class="fr-x-fieldset--raw" data-form-reveal-target="formControl">
                            <fieldset class="fr-fieldset" aria-labelledby="">
                                {{ form_row(form.file, {
                                    group_class: 'fr-input-group',
                                }) }}
                            </fieldset>
                        </fieldset>
                    </div>
                </div>
            </div>
            <div class="fr-fieldset__element" data-form-reveal-target="section" {% if form.allRegulationOrderStorage.vars.value != 'url' %}hidden{% endif %}>
                <div class="app-card app-card--content-only app-someVehicles__form">
                    <div class="app-card__content fr-grid-row">
                        <fieldset class="fr-x-fieldset--raw" data-form-reveal-target="formControl">
                            <fieldset class="fr-fieldset" aria-labelledby="">
                                {{ form_row(form.title, { 
                                    group_class: 'fr-input-group',
                                    widget_class: 'fr-input app-form-title',
                                    row_attr: {
                                        class: 'fr-col-12',
                                    }
                                }) }}
                                {{ form_row(form.url, { 
                                    group_class: 'fr-input-group',
                                    widget_class: 'fr-input app-form-title',
                                    row_attr: {
                                        class: 'fr-col-12',
                                    }
                                }) }}
                            </fieldset>
                        </fieldset>
                    </div>
                </div>
            </div>
            <ul class="fr-fieldset__element fr-btns-group fr-btns-group--inline fr-btns-group--right fr-mt-3w">
                    <li>
                        <a href="{{ baseUrl ~ path('app_regulations_list') }}" role="button" class="fr-btn fr-btn--tertiary fr-btns-group--right">
                            {{ "common.cancel"|trans }}
                        </a>
                    </li>
                    <li>
                        {{ form_widget(form.save, {attr: {class: 'fr-btn fr-btn-load','data-action': 'change->form-submit#submit',
                        },}) }}
                    </li>
                </ul>
        </fieldset>
    {{form_end(form)}}
</section>


